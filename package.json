{
  "name": "@sibipro/eslint-config",
  "version": "0.0.0",
  "description": "Eslint configuration for sibu web projects",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '*.{js,mjs}' 'src/**/*.{js,mjs}'",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sibipro/eslint-config.git"
  },
  "keywords": [],
  "files": [
    "rules/",
    "index.js",
    "CHANGELOG.md",
    "AUTHORS.md"
  ],
  "author": "Sibi (https://github.com/sibipro)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sibipro/eslint-config/issues"
  },
  "homepage": "https://github.com/sibipro/eslint-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "prettier": "^1.16.4"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "eslint": "^6.4.0"
  }
}
